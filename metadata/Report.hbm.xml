<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false">
  <class name="org.openmrs.module.patientmatching.Report" table="patientmatching_report">
  	<id name="reportId" column="report_id"
			unsaved-value="0">
			<generator class="identity" />
		</id>
  <property name="reportName" column="report_name" length="255" />
  
  <bag name="matchingSetList" cascade="all" >
            <key column="configuration_key"/>
            <one-to-many class="org.openmrs.module.patientmatching.MatchingSet"/>
        </bag>  
  </class>
  </hibernate-mapping>